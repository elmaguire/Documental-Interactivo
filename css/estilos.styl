	width 100%
	.descripcion,
	.redes
		block()
		width 100%
		relative()
		max-width: 100%;
		background: none;
	.descripcion
		height 90%
		h2,
		button
			color negro
	.redes
		color casiblanco
		a
			color casiblanco
		li,
		span,
		ul
			alineados middle
		li
			margin-left 0.5rem
			span
				font-size 1.6rem
		ul
			margin-right 0.5rem

.seccion-principal
	block()
	height unquote(' calc( 100% - 40px ) ')
	overflow hidden
	width 100%
	*zoom: 1
	.sensible
		// bg rgba(0,0,0,0.1)
		absolute()
		height 75%
		top 12%
		width 10%
		z-index 22
		&:hover
			cursor pointer
			&:before
				color rgba(255,255,255,0.5)
				sombrat rgba(0,0,0,0.5) 0px 0px 4px
		&:before
			absolute()
			color rgba(255,255,255,0.2)
			font-size 3.5rem
			left 60%;
			top 50%;
			transform translate(-50%, -50%)
			-ms-transform: translate(-50%, -50%)
			-webkit-transform translate(-50%, -50%)
		&.izq
			left 0
			rotar(180deg)
		&.der
			right 0
	.diapositiva
		block()
		relative()
		-webkit-transition: all 0.3s ease;
		-moz-transition: all 0.3s ease;
		transition: all 0.3s ease;
		&.anterior
			-webkit-transform translateX(10%)
			transform translateX(10%)
			z-index 20
		&.siguiente
			-webkit-transform translateX(-10%)
			transform translateX(-10%)
			z-index 20
	.descripcion
		-webkit-transition: all 0.3s ease;
		-moz-transition: all 0.3s ease;
		transition: all 0.3s ease;
		h3,
		p,
		.int-cancel
			-webkit-transition: all 0.3s ease;
			-moz-transition: all 0.3s ease;
			transition: all 0.3s ease;
		.int-cancel
			font-size 2rem
			cursor pointer
			sombrat rgba(0,0,0,1) 0px 0px 5px
			block()
			width 1em
			-webkit-transform: translateZ(0);
			transform: translateZ(0);
			-webkit-transition: all 0.2s;
			-moz-transition: all 0.2s;
			transition: all 0.2s;
			&:hover,
			&:focus,
			&:active
				-webkit-animation: pop_giro 0.3s ease 1;
				animation: pop_giro 0.3s ease 1;
		.textos.arriba
			margin-top 3.5rem
